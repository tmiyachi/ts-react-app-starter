    "@types/mini-css-extract-plugin": "^2.5.0",
    "@types/node": "^14.18.12",
    "@types/react": "^17.0.42",
    "@types/react-dom": "^17.0.14",
    "@types/webpack-dev-server": "^4.7.1",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "babel-loader": "^8.2.4",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.11.0",
    "eslint-cli": "^1.1.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.6.0",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "stylelint": "^14.6.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-scss": "^4.2.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "typescript-tuple": "^5.0.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.ts",
    "build:dev": "webpack --config webpack.dev.ts --watch",
    "start": "webpack serve --config webpack.dev.ts",
    "lint:eslint": "eslint src/**/*.{ts,tsx,js,jsx}",
    "lint:stylelint": "stylelint src/**/*.{css,scss}",
    "lint:prettier": "prettier src/**/*.{ts,tsx,js,jsx,css,scss} --check",
    "lint": "run-p lint:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:stylelint": "npm run lint:stylelint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "fix": "run-s fix:*"
  },
  "private": true,
  "keywords": [],
  "author": "tmiyachi",
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
